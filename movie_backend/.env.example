# Flask Backend Environment Example

# Supabase project URL
SUPABASE_URL=

# Service Role key (DO NOT expose to frontend)
SUPABASE_SERVICE_ROLE_KEY=

# Optional anon key (not used for writes on backend)
SUPABASE_ANON_KEY=

# CORS origin for frontend (e.g., http://localhost:3000)
FRONTEND_ORIGIN=http://localhost:3000

# Optional site URL (used by auth redirect docs or future features)
SITE_URL=http://localhost:3000

# Flask environment
FLASK_ENV=development

# Port for backend (container may set this externally)
PORT=3001
